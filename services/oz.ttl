#################################################################
#    Individuals
#################################################################
_:targetHypervisorVLB rdf:type im:VariableSpecification ;
	im:defines nm:Host .
	# Can add extra information here.
	# Either a specific host, or simply requirements
	# for the host to meet.
	# model allows for granular specification
	# in addition to supporting high-level.

_:targetHypervisorSBE rdf:type im:VariableSpecification ;
	im:defines nm:Host .

_:targetNetwork rdf:type im:VariableSpecification ;
	im:defines nm:Network ;
	nm:bandwidth "100000000"^^xsd:double.

_:replicatorOne rdf:type im:Replicator ;
	im:replicates :wodanSBE_VDU .

###  http://raw.githubusercontent.com/Guilvareux/ontologies/main/examples/tmf_example/service#wodanVLB
:wodanVLB rdf:type owl:NamedIndividual , im:FunctionSpecification ;
			  im:requiresDeploymentUnit :wodanVLB_VDU ;
			im:requiresRole nm:LoadBalanceFunction .


###  http://raw.githubusercontent.com/Guilvareux/ontologies/main/examples/tmf_example/service#wodanVLB_VDU
:wodanVLB_VDU rdf:type owl:NamedIndividual , im:VirtualMachineSpecification ;
		 im:requiresHost _:targetHypervisorVLB ;
         im:minCPURequirement 6 ;
         im:minMemoryRequirement "1000000000"^^xsd:double ;
         im:minStorageRequirement "10000000000"^^xsd:double ;
         im:connectsToNetwork _:targetNetwork .


###  http://raw.githubusercontent.com/Guilvareux/ontologies/main/examples/tmf_example/service#wodanSBE
:wodanSBE rdf:type owl:NamedIndividual , im:FunctionSpecification ;
			im:requiresDeploymentUnit :wodanSBE_VDU ;
			im:requiresRole nm:BackendFunction .


###  http://raw.githubusercontent.com/Guilvareux/ontologies/main/examples/tmf_example/service#wodanSBE_VDU
:wodanSBE_VDU rdf:type owl:NamedIndividual , im:VirtualMachineSpecification ;
		 im:requiresHost _:targetHypervisorSBE ;
         im:minCPURequirement 4 ;
         im:minMemoryRequirement "1000000000"^^xsd:double ;
         im:minStorageRequirement "10000000000"^^xsd:double ;
         im:connectsToNetwork _:targetNetwork .


:EmeraldFunctionChain rdf:type owl:NamedIndividual ,
					im:FunctionChainSpecification ;
					im:requiresFunction :wodanVLB , :wodanSBE ;
					im:minReplication 3 ;
					im:replication 3 .


:EmeraldService rdf:type owl:NamedIndividual ,
                    im:ServiceSpecification ;
					im:requiresFunctionChain :EmeraldFunctionChain .


